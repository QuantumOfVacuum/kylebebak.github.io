---
layout: nil
---

[
{% for post in site.posts %}
  {
    "title": "{{ post.title }}",
    "url": "{{ site.url }}{{ post.url }}",
    "date": "{{ post.date }}",
    "categories": {{ post.categories | split:' ' }},
    "tags": {{ post.tags | split:' ' }}
  } {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]
